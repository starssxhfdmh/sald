{
    "Function": {
        "prefix": "fun",
        "body": [
            "fun ${1:name}(${2:params}) {",
            "\t$0",
            "}"
        ],
        "description": "Function declaration"
    },
    "Async Function": {
        "prefix": "afun",
        "body": [
            "async fun ${1:name}(${2:params}) {",
            "\t$0",
            "}"
        ],
        "description": "Async function declaration"
    },
    "Class": {
        "prefix": "class",
        "body": [
            "class ${1:Name} {",
            "\tfun init(self${2:, params}) {",
            "\t\t$0",
            "\t}",
            "}"
        ],
        "description": "Class declaration"
    },
    "Class with Extends": {
        "prefix": "classex",
        "body": [
            "class ${1:Name} extends ${2:Parent} {",
            "\tfun init(self${3:, params}) {",
            "\t\tsuper.init(${4:args})",
            "\t\t$0",
            "\t}",
            "}"
        ],
        "description": "Class with inheritance"
    },
    "Namespace": {
        "prefix": "namespace",
        "body": [
            "namespace ${1:Name} {",
            "\t$0",
            "}"
        ],
        "description": "Namespace declaration"
    },
    "Enum": {
        "prefix": "enum",
        "body": [
            "enum ${1:Name} {",
            "\t${2:Variant1},",
            "\t${3:Variant2}",
            "}"
        ],
        "description": "Enum declaration"
    },
    "For Loop": {
        "prefix": "for",
        "body": [
            "for ${1:item} in ${2:items} {",
            "\t$0",
            "}"
        ],
        "description": "For-in loop"
    },
    "For Range": {
        "prefix": "forr",
        "body": [
            "for ${1:i} in ${2:0}..<${3:10} {",
            "\t$0",
            "}"
        ],
        "description": "For loop with range"
    },
    "While Loop": {
        "prefix": "while",
        "body": [
            "while ${1:condition} {",
            "\t$0",
            "}"
        ],
        "description": "While loop"
    },
    "If Statement": {
        "prefix": "if",
        "body": [
            "if ${1:condition} {",
            "\t$0",
            "}"
        ],
        "description": "If statement"
    },
    "If Else": {
        "prefix": "ife",
        "body": [
            "if ${1:condition} {",
            "\t$2",
            "} else {",
            "\t$0",
            "}"
        ],
        "description": "If-else statement"
    },
    "Switch": {
        "prefix": "switch",
        "body": [
            "let ${1:result} = switch ${2:value} {",
            "\t${3:pattern} -> ${4:result}",
            "\tdefault -> ${0:fallback}",
            "}"
        ],
        "description": "Switch expression"
    },
    "Try Catch": {
        "prefix": "try",
        "body": [
            "try {",
            "\t$0",
            "} catch (${1:e}) {",
            "\tConsole.println(${1:e})",
            "}"
        ],
        "description": "Try-catch block"
    },
    "Lambda": {
        "prefix": "lam",
        "body": "|${1:x}| ${0:expr}",
        "description": "Lambda expression"
    },
    "Lambda Block": {
        "prefix": "lamb",
        "body": [
            "|${1:params}| {",
            "\t$0",
            "}"
        ],
        "description": "Lambda with block body"
    },
    "Async Lambda": {
        "prefix": "alam",
        "body": "async |${1:x}| await ${0:expr}",
        "description": "Async lambda expression"
    },
    "HTTP Server": {
        "prefix": "httpserver",
        "body": [
            "let server = Http.Server()",
            "",
            "server.get(\"/\", |req| {",
            "\treturn { body: \"Hello World\" }",
            "})",
            "",
            "server.listen(${1:8080})"
        ],
        "description": "Basic HTTP server"
    },
    "HTTP Route": {
        "prefix": "route",
        "body": [
            "server.${1|get,post,put,delete,patch|}(\"${2:/path}\", |req| {",
            "\t$0",
            "\treturn { body: \"OK\" }",
            "})"
        ],
        "description": "HTTP route handler"
    },
    "Import": {
        "prefix": "imp",
        "body": "import \"${1:module}\"",
        "description": "Import statement"
    },
    "Import As": {
        "prefix": "impas",
        "body": "import \"${1:module}\" as ${2:Alias}",
        "description": "Import with alias"
    },
    "Console.println": {
        "prefix": "log",
        "body": "Console.println($0)",
        "description": "Print to console"
    },
    "Console.println Format": {
        "prefix": "logf",
        "body": "Console.println($$\"${1:message}: {${0:value}}\")",
        "description": "Print formatted string"
    },
    "Let Variable": {
        "prefix": "let",
        "body": "let ${1:name} = ${0:value}",
        "description": "Variable declaration"
    },
    "Const": {
        "prefix": "const",
        "body": "const ${1:NAME} = ${0:value}",
        "description": "Constant declaration"
    },
    "Await": {
        "prefix": "aw",
        "body": "await ${0:promise}",
        "description": "Await expression"
    },
    "Array Map": {
        "prefix": "map",
        "body": "${1:arr}.map(|${2:x}| ${0:expr})",
        "description": "Array map"
    },
    "Array Filter": {
        "prefix": "filter",
        "body": "${1:arr}.filter(|${2:x}| ${0:condition})",
        "description": "Array filter"
    },
    "Array Reduce": {
        "prefix": "reduce",
        "body": "${1:arr}.reduce(|${2:acc}, ${3:x}| ${4:expr}, ${0:initial})",
        "description": "Array reduce"
    },
    "File Read": {
        "prefix": "fread",
        "body": "let ${1:content} = await File.read(\"${0:path}\")",
        "description": "Read file async"
    },
    "File Write": {
        "prefix": "fwrite",
        "body": "await File.write(\"${1:path}\", ${0:content})",
        "description": "Write file async"
    },
    "JSON Parse": {
        "prefix": "jsonp",
        "body": "let ${1:data} = Json.parse(${0:str})",
        "description": "Parse JSON string"
    },
    "JSON Stringify": {
        "prefix": "jsons",
        "body": "Json.stringify(${1:obj}${2:, 2})",
        "description": "Convert to JSON string"
    }
}
