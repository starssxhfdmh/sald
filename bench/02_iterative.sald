// Benchmark: Iterative Sum
// Tests: Loop performance, basic arithmetic

fun iterative_sum(n) {
    let sum = 0
    let i = 0
    while i < n {
        sum += i
        i += 1
    }
    return sum
}

let start = Timer.now()
let result = iterative_sum(1000000)
let elapsed = Timer.now() - start

Console.println($"Sum(0..1000000) = {result}")
Console.println($"Time: {elapsed}ms")
